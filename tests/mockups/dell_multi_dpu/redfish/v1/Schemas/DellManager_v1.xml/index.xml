    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="DellManager.v1_2_0">
        <Annotation Term="Redfish.OwningEntity" String="Dell"/>
        <Annotation Term="OData.Description" String="This version was created to add DelliDRACCard navigation property and RemoteSystemLogs property."/>
        <Annotation Term="OData.LongDescription" String="This version was created to add DelliDRACCard navigation property and RemoteSystemLogs property."/>
        <ComplexType Name="DellManager" BaseType="DellManager.DellManager">
			      <NavigationProperty Name="DelliDRACCard" Type="DelliDRACCard.DelliDRACCard" ContainsTarget="true" Nullable="false">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
                <Annotation Term="OData.Description" String="A resource representation of DelliDRACCard."/>
                <Annotation Term="OData.LongDescription" String="A resource representation of DelliDRACCard."/>
                <Annotation Term="OData.AutoExpand"/>
            </NavigationProperty>
            <Property Name="RemoteSystemLogs" Type="DellManager.v1_2_0.RemoteSystemLogs" Nullable="false">
                <Annotation Term="OData.Description" String="TLS based remote syslog configurations."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain the TLS based remote syslog configurations."/>
            </Property>
        </ComplexType>

        <ComplexType Name="RemoteSystemLogs">
            <Property Name="HTTPS" Type="DellManager.v1_2_0.HTTPS" Nullable="false">
                <Annotation Term="OData.Description" String="TLS based remote syslog configurations and client trust certificate."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain the TLS based remote syslog configurations and client trust certificate."/>
            </Property>
            <Property Name="CA" Type="DellManager.v1_2_0.CA" Nullable="false">
                <Annotation Term="OData.Description" String="TLS based remote syslog CA certificate."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain the TLS based remote syslog CA certificate."/>
            </Property>
        </ComplexType>

        <ComplexType Name="HTTPS">
            <NavigationProperty Name="Certificates" Type="CertificateCollection.CertificateCollection" ContainsTarget="true">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
                <Annotation Term="OData.Description" String="The link to a collection of client trust certificates used for TLS support for RemoteSystemLogs."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain a link to a Resource Collection of type CertificateCollection."/>
                <Annotation Term="OData.AutoExpandReferences"/>
            </NavigationProperty>
            <Property Name="SecureSysLogEnable" Type="DellManager.v1_2_0.SecureSysLogEnable" Nullable="false">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
                <Annotation Term="OData.Description" String="An indication of whether TLS based remote syslog is enabled."/>
                <Annotation Term="OData.LongDescription" String="This property shall indicate whether TLS based remote syslog is enabled or not. Enabled indicates TLS based remote syslog is enabled. Disabled indicates TLS based remote syslog is disabled. By default TLS support is disabled."/>
            </Property>
            <Property Name="SecurePort" Type="Edm.Int64" Nullable="false">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
                <Annotation Term="OData.Description" String="Specifies the secure remote syslog port number."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain the secure remote syslog port number. The default port value is 6514. The port value should be an integral value from 1 to 65535."/>
                <Annotation Term="Validation.Minimum" Int="1"/>
                <Annotation Term="Validation.Maximum" Int="65535"/>
            </Property>
             <Property Name="SecureClientAuth" Type="DellManager.v1_2_0.SecureClientAuth" Nullable="false">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
                <Annotation Term="OData.Description" String="The identity authentication option used by iDRAC for TLS based remote syslog."/>
                <Annotation Term="OData.LongDescription" String="This property shall indicate the identity authentication option used by iDRAC for TLS based remote syslog. By default iDRAC will use anonymous idenity which can be overriden by generating a signed trust certificate to prove iDRAC's authenticity."/>
            </Property>
            <Property Name="SecureServers" Type="Collection(Edm.String)" Nullable="false">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/ReadWrite"/>
                <Annotation Term="OData.Description" String="Name of the TLS supported remote syslog servers."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain the list of all TLS supported remote syslog servers."/>
            </Property>
        </ComplexType>

        <ComplexType Name="CA">
            <NavigationProperty Name="Certificates" Type="CertificateCollection.CertificateCollection" ContainsTarget="true">
                <Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
                <Annotation Term="OData.Description" String="The link to a collection of CA certificates used for TLS support for RemoteSystemLogs."/>
                <Annotation Term="OData.LongDescription" String="This property shall contain a link to a Resource Collection of type CertificateCollection."/>
                <Annotation Term="OData.AutoExpandReferences"/>
            </NavigationProperty>
        </ComplexType>

        <EnumType Name="SecureSysLogEnable">
            <Member Name="Enabled">
                <Annotation Term="OData.Description" String="TLS based remote syslog is enabled."/>
            </Member>
            <Member Name="Disabled">
                <Annotation Term="OData.Description" String="TLS based remote syslog is disabled."/>
            </Member>
        </EnumType>

        <EnumType Name="SecureClientAuth">
            <Member Name="Anonymous">
                <Annotation Term="OData.Description" String="TLS based remote syslog with anonymous client authentication."/>
            </Member>
            <Member Name="Certificate">
                <Annotation Term="OData.Description" String="TLS based remote syslog with certificate based client trust authentication"/>
            </Member>
        </EnumType>
      </Schema>
      <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="DellManager.v1_3_0">
        <Annotation Term="Redfish.OwningEntity" String="Dell"/>
        <Annotation Term="OData.Description" String="This version was created to add DellJobCollection navigation property."/>
        <Annotation Term="OData.LongDescription" String="This version was created to add DellJobCollection navigation property."/>
