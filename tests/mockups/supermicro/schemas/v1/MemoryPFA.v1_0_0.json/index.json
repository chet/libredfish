{
    "definitions": {
        "MemoryPFA": {
            "description": "The base type for resources and members that can be linked to.",
            "required": [
                "Id",
                "Name"
            ],
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/type"
                },
                "MemoryPfaNext": {
                    "readonly": false,
                    "type": "string",
                    "description": "Next status set by tools would like to change the state.",
                    "longDescription": "The property shall contain the next status set by tools would like to change the state."
                },
                "Description": {
                    "readonly": true,
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "AlertId": {
                    "readonly": false,
                    "type": [
                        "number",
                        "null"
                    ],
                    "description": "Forward SELs at one specific Alert Id registered by tool.",
                    "longDescription": "The property shall contain the forward SELs at one specific Alert Id registered by tool."
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/id"
                },
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/context"
                },
                "MemoryPfaInit": {
                    "readonly": true,
                    "type": "string",
                    "description": "The current state from Bios HII value.",
                    "longDescription": "The property shall contain the current state from Bios HII value."
                },
                "Oem": {
                    "longDescription": "This property shall contain the OEM extensions.  All values for properties that this object contains shall conform to the Redfish Specification-described requirements.",
                    "description": "The OEM extension property.",
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem"
                },
                "Id": {
                    "readonly": true,
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id"
                },
                "Name": {
                    "readonly": true,
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name"
                }
            }
        }
    },
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_4_0.json",
    "$ref": "#/definitions/MemoryPFA",
    "copyright": "Copyright 2014-2017 Distributed Management Task Force, Inc. (DMTF). For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright",
    "title": "#MemoryPFA.v1_0_0.MemoryPFA"
}