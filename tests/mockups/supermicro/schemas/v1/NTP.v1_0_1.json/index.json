{
    "definitions": {
        "NTP": {
            "description": "Prepare for the next version of NTP schema.",
            "required": [
                "Id",
                "Name"
            ],
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/type"
                },
                "Description": {
                    "readonly": true,
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "NTPEnable": {
                    "readonly": false,
                    "type": "boolean",
                    "description": "NTP\uff08Network Time Protocol) service is enabled or not",
                    "longDescription": "If NTP service is enabled, synchronising time over a network."
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/id"
                },
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/context"
                },
                "PrimaryNTPServer": {
                    "readonly": false,
                    "type": [
                        "string",
                        "null"
                    ],
                    "description": "Primary NTP server ip address",
                    "longDescription": "The value of this property shall be match the format of IP address or domain name."
                },
                "Oem": {
                    "longDescription": "This property shall contain the OEM extensions.  All values for properties that this object contains shall conform to the Redfish Specification-described requirements.",
                    "description": "The OEM extension property.",
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem"
                },
                "DaylightSavingTime": {
                    "readonly": false,
                    "type": [
                        "boolean",
                        "null"
                    ],
                    "description": "in Daylight Saving Time or not",
                    "longDescription": "If it's in daylight saving time, the practice of advancing clocks during summer months by one hour so that in the evening day light is experienced an hour longer."
                },
                "SecondaryNTPServer": {
                    "readonly": false,
                    "type": [
                        "string",
                        "null"
                    ],
                    "description": "Secondary NTP server ip address",
                    "longDescription": "The value of this property shall be match the format of IP address or domain name."
                },
                "Id": {
                    "readonly": true,
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id"
                },
                "Name": {
                    "readonly": true,
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name"
                }
            }
        }
    },
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_4_0.json",
    "$ref": "#/definitions/NTP",
