{
    "definitions": {
        "Mount": {
            "description": "The actions mounts/unmounts an image file.",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "longDescription": "This defines the name of the custom action supported when used in conjunction with a POST operation to this resource. When issued, this operation will perform a mount/unmount action to the virtual media config supplied as a parameter.",
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "target": {
                    "type": "string",
                    "description": "Link to invoke action",
                    "format": "uri"
                },
                "title": {
                    "type": "string",
                    "description": "Friendly action name"
                }
            }
        },
