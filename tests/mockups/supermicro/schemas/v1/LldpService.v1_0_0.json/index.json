{
    "definitions": {
        "LldpService": {
            "description": "The base type for resources and members that can be linked to.",
            "required": [
                "Id",
                "Name"
            ],
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/type"
                },
                "Description": {
                    "readonly": true,
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/id"
                },
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.v4_0_2.json#/definitions/context"
                },
                "LLDPReceive": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Port.v1_4_0.json#/definitions/LLDPReceive"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "LLDP data being received on this link.",
                    "longDescription": "This property shall contain the LLDP data being received on this link."
                },
                "LLDPEnabled": {
                    "readonly": false,
                    "type": "boolean",
                    "description": "Enable/disable LLDP for this port.",
                    "longDescription": "This property shall contain the state indicating whether to enable LLDP for a port. If LLDP is disabled at the adapter level, this property shall be ignored."
                },
                "Oem": {
                    "longDescription": "This property shall contain the OEM extensions.  All values for properties that this object contains shall conform to the Redfish Specification-described requirements.",
                    "description": "The OEM extension property.",
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem"
                },
                "LLDPTransmit": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Port.v1_4_0.json#/definitions/LLDPTransmit"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "LLDP data being transmit on this link.",
                    "longDescription": "This property shall contain the LLDP data being transmit on this link."
                },
                "Id": {
                    "readonly": true,
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id"
                },
                "Name": {
                    "readonly": true,
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name"
                }
            }
        }
    },
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_4_0.json",
    "$ref": "#/definitions/LldpService",
