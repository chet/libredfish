{
    "definitions": {
        "ServiceRoot": {
            "additionalProperties": true,
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "type": "object",
            "properties": {
                "DumpService": {
                    "readonly": true,
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/DumpService.json#/definitions/DumpService"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "The property represents the Dump Service.",
                    "longDescription": "The value of this property shall contain the Dump Service."
                }
            }
        }
    },
