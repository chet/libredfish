                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/NTP.json#/definitions/NTP"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "This is a reference to a collection of Logs used by the manager.",
                    "longDescription": "The value of this property shall contain a reference to a collection of type NTP which are for the use of this manager."
                },
                "SmartPower": {
                    "readonly": true,
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/SmartPower.json#/definitions/SmartPower"
