        "Option": {
            "enum": [
                "PreserveUser",
                "ResetToADMIN"
            ],
            "type": "string"
        },
        "SmcManagerExtensions": {
            "description": "The base type for resources and members that can be linked to.",
            "required": [
                "Id",
                "Name"
            ],
            "patternProperties": {
