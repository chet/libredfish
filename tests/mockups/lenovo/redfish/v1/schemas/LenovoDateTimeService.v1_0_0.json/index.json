{
    "$id": "http://redfish.dmtf.org/schemas/v1/LenovoDateTimeService.v1_0_0.json",
    "$ref": "#/definitions/LenovoDateTimeService",
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema-v1.json",
    "copyright": "Copyright 2014-2021 DMTF. For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright",
    "definitions": {
        "Actions": {
            "additionalProperties": false,
            "description": "The available actions for this resource.",
            "longDescription": "This type shall contain the available actions for this resource.",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_]*$": {
                    "description": "This property shall specify a valid odata or Redfish property.",
                    "type": [
                        "array",
                        "boolean",
                        "integer",
                        "number",
                        "null",
                        "object",
                        "string"
                    ]
                }
            },
            "properties": {
                "#LenovoDateTimeService.ImmediatelySync": {
                    "$ref": "#/definitions/ImmediatelySync"
                }
            },
            "type": "object"
        },
        "DateTimeSettingMethod": {
            "enum": [
                "SyncwithHost",
                "SyncwithNTP"
            ],
            "enumDescriptions": {
                "SyncwithHost": "The time is got from the host.",
                "SyncwithNTP": "User gives out the NTP server to synchronize the data and time."
            },
            "type": "string"
        },
        "HostTimeFormat": {
            "enum": [
                "Local",
                "UTC"
            ],
            "enumDescriptions": {
                "Local": "The time is got from the host is local(example:Windows) time.",
                "UTC": "The time is got from the host is UTC(example:Linux) time."
            },
            "type": "string"
        },
        "ImmediatelySync": {
            "additionalProperties": false,
            "description": "Use the action to request an immediate synchronization with NTP server.",
            "longDescription": "If select to synchronize the date and time with NTP server, the action can be used to make an immediate request.",
            "parameters": {},
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_]*$": {
                    "description": "This property shall specify a valid odata or Redfish property.",
                    "type": [
                        "array",
                        "boolean",
                        "integer",
                        "number",
                        "null",
                        "object",
                        "string"
                    ]
                }
            },
            "properties": {
                "target": {
                    "description": "Link to invoke action",
                    "format": "uri-reference",
                    "type": "string"
                },
                "title": {
                    "description": "Friendly action name",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "LenovoDateTimeService": {
            "additionalProperties": false,
            "description": "This resource represents the Date and Time service for the resource or service to which it is associated.",
            "longDescription": "This resource shall be used to represent a date/time service of BMC.",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_]*$": {
                    "description": "This property shall specify a valid odata or Redfish property.",
                    "type": [
                        "array",
                        "boolean",
                        "integer",
                        "number",
                        "null",
                        "object",
                        "string"
                    ]
                }
            },
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata-v4.json#/definitions/context"
                },
                "@odata.etag": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata-v4.json#/definitions/etag"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata-v4.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata-v4.json#/definitions/type"
                },
                "Actions": {
                    "$ref": "#/definitions/Actions",
                    "description": "This object is used to define Lenovo date/time actions.",
                    "longDescription": "The Actions property shall contain the available actions for this resource."
                },
                "AutoDST": {
                    "description": "Specify whether the manager clock will automatically adjust when Daylight Saving Time(DST) changes.",
                    "longDescription": "The optional values are 'true' or 'false'. If the value is 'true', the manager will perform an automatic Daylight Saving Time update.",
                    "readonly": false,
                    "type": [
                        "boolean",
                        "null"
                    ]
                },
                "DateTime": {
                    "description": "Indicate current Date and Time of manager, the format should be Month/Day/Year Hour:Minute:Second.",
                    "longDescription": "The value of this property shall represent the current DateTime value of Manager, with offset from UTC.",
                    "readonly": true,
                    "type": "string"
                },
                "Description": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "readonly": true
                },
                "Frequency": {
                    "description": "Specify the Synchronization frequency in Munites.",
                    "longDescription": "Specify the interval between synchronization requests when 'Synchronize with an NTP server' is selected.",
                    "readonly": false,
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "HostTimeFormat": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/HostTimeFormat"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "Format of time obtained from host.",
                    "longDescription": "The optional values are 'Local(example:Windows)' or 'UTC(example:Linux)'. If the value is 'UTC', the UTCOffset can update."
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "NTPServerAddresses": {
                    "description": "Indicate the NTP server for lock synchronization, the address should be at least valid in format",
                    "items": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "longDescription": "TUse this field to specify the names of up to 4 NTP servers to be used for clock synchronization when 'Synchronize with an NTP server' is selected.",
                    "readonly": false,
                    "type": "array"
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "The OEM extension property.",
                    "longDescription": "This property shall contain the OEM extensions.  All values for properties that this object contains shall conform to the Redfish Specification-described requirements."
                },
                "SettingMethod": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/DateTimeSettingMethod"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "Indicate how the manager Date and Time are updated.",
                    "longDescription": "Indicate how the manager Date and Time should be set.",
                    "readonly": false
                },
                "UTCOffset": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/UTCOffsetCollection"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "Indicate UTC offset for the manager.",
                    "longDescription": "Specify the offset from Universal Time Coordinated(UTC) corresponding to the time zone where this system is located.The format  should be UTC +/- Hour:Mintue.",
                    "readonly": false
                }
            },
            "required": [
                "@odata.id",
                "@odata.type",
                "Id",
                "Name"
            ],
            "type": "object"
        },
