                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata-v4.json#/definitions/type"
                },
                "Actions": {
                    "$ref": "#/definitions/Actions",
                    "description": "The manufacturer of the firmware.",
                    "longDescription": "The value of this property shall be the manufacturer of the firmware.",
                    "readonly": true
                },
                "BMAppStatus": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/BMAppStatus"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "description": "Represents the bare metal application status.",
                    "longDescription": "The value of this property shall represent the bare metal application status.",
                    "readonly": true
                },
                "BMU_Credential": {
                    "$ref": "#/definitions/BMU_Credential",
                    "description": "The manufacturer of the firmware.",
                    "longDescription": "The value of this property shall be the manufacturer of the firmware."
                },
                "ChangeHistory": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/LogService.json#/definitions/LogService",
                    "description": "Represents firmware change history.",
                    "longDescription": "This property shall be a reference to LogService.json used to represent firmware change history.",
                    "readonly": true
                },
                "Description": {
                    "anyOf": [
                        {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "readonly": true
                },
                "FreeRdocSpaceInKB": {
                    "description": "Specify the available size of RDOC space(KBytes).",
                    "longDescription": "The value of this property shall reresent the available size of RDOC space(KBytes).",
                    "readonly": true,
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "The OEM extension property.",
                    "longDescription": "This property shall contain the OEM extensions.  All values for properties that this object contains shall conform to the Redfish Specification-described requirements."
                },
                "Started": {
                    "description": "Indicate whether the bare metal update action started.",
                    "longDescription": "The value of this property shall represent the bare metal update action started or not.",
                    "type": [
                        "boolean",
                        "null"
                    ]
                }
            },
            "required": [
                "@odata.id",
                "@odata.type",
                "Id",
                "Name"
            ],
            "type": "object"
        },
