        "BMAppStatus": {
            "enum": [
                "NotReady",
                "Booting",
                "FullyBooted",
                "PendingAndAwaitingNextServerBoot",
                "NetworkAvailable",
                "FileSystemInit",
                "FailedToBootBMU",
                "Unknown"
            ],
            "type": "string"
        },
        "BMU_Credential": {
            "additionalProperties": false,
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_]*$": {
                    "description": "This property shall specify a valid odata or Redfish property.",
