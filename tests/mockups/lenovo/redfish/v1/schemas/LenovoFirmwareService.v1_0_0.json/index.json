        "StartBareMetalConnection": {
            "additionalProperties": false,
            "parameters": {
                "BareMetalUpdateData": {
                    "description": "Optional data from FSM, up to 32KB.",
                    "type": "string"
                },
                "BootTimeoutInMin": {
                    "description": "The communication boot timeout when bmu is booting(by minutes).",
                    "type": "integer"
                },
                "ExternalPort": {
                    "description": "The port IMM listens to on the management network, default 6990.",
                    "type": "integer"
                },
                "ImageName": {
                    "description": "The filename of RDOC image uploaded to XCC sftp server.",
                    "requiredParameter": true,
                    "type": "string"
                },
                "TargetServerIP": {
                    "description": "The target server IP which will be used by SALIE OS to download the rootfs image. If this is not specified, only uni-direction (external-BMU OS) port forwarding is enabled; If specified, bi-direction port forwarding will be enabled.",
                    "type": "string"
                },
                "TimeoutInMin": {
                    "description": "The communication watchdog timeout after bmu ready(by minutes).",
                    "type": "integer"
                },
                "USBPort": {
                    "description": "The port on the Ethernet over USB interface IMM will forward traffic to, default 6990.",
                    "type": "integer"
                }
